{
  "name": "@safeheron/mpcsnap",
  "version": "2.0.0-beta.1",
  "description": "MetaMask MPCSnap",
  "main": "dist/index.ts",
  "keywords": [
    "Safeheron",
    "TSS",
    "MPC",
    "MetaMask",
    "MetaMask Flask",
    "Snap",
    "ETH"
  ],
  "author": "Yann<yanjie@safeheron.com>",
  "files": [
    "dist/",
    "snap.manifest.json"
  ],
  "scripts": {
    "clean": "rm -rf dist && rm -rf artifacts",
    "prebuild": "yarn run clean",
    "build": "webpack --progress --config webpack.config.ts",
    "manifest": "mm-snap manifest --fix",
    "eval": "mm-snap eval -b dist/bundle.js --verboseErrors",
    "serve": "yarn run clean && concurrently \"webpack --watch\" \"mm-snap serve\"",
    "test": "jest ./test/**/*.test.ts",
    "cover": "jest ./test/**/*.test.ts --coverage",
    "lint": "eslint --fix --ext .ts,.tsx,.js src/"
  },
  "dependencies": {
    "@ethersproject/bytes": "^5.6.1",
    "@metamask/eth-sig-util": "^6.0.0",
    "@metamask/keyring-api": "^0.1.3",
    "@metamask/snaps-ui": "0.35.2-flask.1",
    "@safeheron/mpc-wasm-sdk": "0.1.0-rc.1",
    "@safeheron/mpcsnap-types": "workspace:^",
    "ethers": "^5.6.9",
    "loadsh": "^0.0.4",
    "stream-browserify": "^3.0.0",
    "superstruct": "^1.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.18.6",
    "@metamask/snaps-cli": "0.33.0-flask.1",
    "@metamask/snaps-types": "0.35.2-flask.1",
    "@metamask/snaps-webpack-plugin": "0.33.0-flask.1",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.0.0",
    "@types/sinon": "^10.0.12",
    "@types/uuid": "^9.0.2",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "crypto-browserify": "^3.12.0",
    "hook-shell-script-webpack-plugin": "^0.1.4",
    "jest": "^27.4.7",
    "process": "^0.11.10",
    "sinon": "^14.0.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "webassembly-loader": "^1.1.0",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  }
}
