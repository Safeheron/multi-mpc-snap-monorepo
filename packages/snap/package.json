{
  "name": "@safeheron/mpcsnap",
  "version": "2.4.5-rc.0",
  "description": "MetaMask MPCSnap",
  "main": "dist/index.ts",
  "keywords": [
    "Safeheron",
    "TSS",
    "MPC",
    "MetaMask",
    "MetaMask Flask",
    "Snap",
    "ETH"
  ],
  "author": "Yann<yanjie@safeheron.com>",
  "files": [
    "dist/",
    "snap.manifest.json"
  ],
  "repository": {
    "url": "https://github.com/safeheron/multi-mpc-snap-monorepo",
    "type": "github"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf artifacts",
    "prebuild": "yarn run clean",
    "build:watch": "webpack --progress --config webpack.config.ts --watch --mode development",
    "build": "webpack --progress --config webpack.config.ts --mode production",
    "build:dev": "NODE_ENV=development RUN_ENV=dev yarn run build:watch",
    "build:test": "NODE_ENV=production RUN_ENV=test yarn run build",
    "build:prod": "NODE_ENV=production RUN_ENV=prod yarn run build",
    "manifest": "mm-snap manifest --fix",
    "eval": "mm-snap eval -b dist/bundle.js --verboseErrors",
    "serve": "yarn run clean && concurrently \"npm run build:dev\" \"mm-snap serve\"",
    "test": "jest ./test/**/*.test.ts",
    "cover": "jest ./test/**/*.test.ts --coverage",
    "lint": "eslint --fix --ext .ts,.tsx,.js src/",
    "publish:test": "yarn run build:test && yarn npm publish --access public --tag beta",
    "publish:rc": "yarn run build:prod && yarn npm publish --access public --tag rc",
    "publish:prod": "yarn run build:prod && npm publish --access public"
  },
  "dependencies": {
    "@ethersproject/bytes": "^5.6.1",
    "@metamask/eth-sig-util": "^7.0.1",
    "@metamask/keyring-api": "^1.0.0",
    "@metamask/snaps-ui": "^2.0.0",
    "@metamask/snaps-utils": "^3.0.0",
    "@safeheron/mpc-wasm-sdk": "0.4.1",
    "@safeheron/mpcsnap-types": "workspace:^",
    "ethers": "^5.6.9",
    "loadsh": "^0.0.4",
    "stream-browserify": "^3.0.0",
    "superstruct": "^1.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.18.6",
    "@metamask/snaps-cli": "^3.0.0",
    "@metamask/snaps-types": "^3.0.0",
    "@metamask/snaps-webpack-plugin": "^3.0.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.0.0",
    "@types/sinon": "^10.0.12",
    "@types/uuid": "^9.0.2",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "crypto-browserify": "^3.12.0",
    "hook-shell-script-webpack-plugin": "^0.1.4",
    "jest": "^27.4.7",
    "process": "^0.11.10",
    "sinon": "^14.0.0",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "webassembly-loader": "^1.1.0",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.9.0"
  }
}
